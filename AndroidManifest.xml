<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.familymail">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/fm_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/fm_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FamilyMail">
        <activity android:name=".ComposeActivity" android:label="Compose" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".MainActivity" android:label="FamilyMail by Akilesh">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>